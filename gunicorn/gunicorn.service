[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=www-data
Group=www-data
WorkingDirectory=/ubuntu/blogprojectdrf
ExecStart=/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/run/gunicorn.sock/ubuntu/blogprojectdrf/blog.wsgi:application

       
[Install]
WantedBy=multi-user.target